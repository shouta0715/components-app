{
  "name": "component-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "preview": "next build && open http://localhost:3000 && next start",
    "start": "next start",
    "lint": "next lint",
    "generate": "pnpm prisma generate",
    "studio": "pnpm prisma studio",
    "studio:test": "dotenv -e .env.test pnpm prisma studio",
    "seed": "dotenv -e .env.development.local pnpm prisma db seed ",
    "new-migrate": "pnpm prisma migrate dev --name",
    "migrate": "dotenv -e .env.development.local pnpm prisma migrate dev",
    "migrate:all": "pnpm migrate && pnpm migrate:test",
    "prepare": "husky install",
    "format": "prettier --write . && eslint --fix .",
    "migrate:test": "dotenv -e .env.test pnpm prisma migrate dev",
    "test": "pnpm migrate:test && pnpm vitest -c ./vitest.config.ts",
    "test:ci": "pnpm migrate:test && dotenv -e .env.test pnpm vitest -c ./vitest.config.ts",
    "test:coverage": "pnpm vitest --ui --coverage",
    "test:ui": "pnpm vitest -c ./vitest.config.ts --ui"
  },
  "dependencies": {
    "@auth/core": "^0.18.4",
    "@auth/prisma-adapter": "^1.0.10",
    "@aws-sdk/client-s3": "^3.465.0",
    "@aws-sdk/s3-presigned-post": "^3.465.0",
    "@aws-sdk/s3-request-presigner": "^3.465.0",
    "@next/env": "^14.0.3",
    "@paralleldrive/cuid2": "^2.2.2",
    "@prisma/client": "^5.7.0",
    "@radix-ui/react-aspect-ratio": "^1.0.3",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-collapsible": "^1.0.3",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-hover-card": "^1.0.7",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-scroll-area": "^1.0.5",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tabs": "^1.0.4",
    "@tanstack/react-query": "^5.14.2",
    "@tanstack/react-query-devtools": "^5.14.5",
    "@types/minify": "^9.1.4",
    "bright": "^0.8.4",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "cmdk": "^0.2.0",
    "copy-to-clipboard": "^3.3.3",
    "embla-carousel-autoplay": "8.0.0-rc15",
    "embla-carousel-react": "8.0.0-rc15",
    "lucide-react": "^0.294.0",
    "next": "14.0.4",
    "next-auth": "5.0.0-beta.4",
    "next-themes": "^0.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.12",
    "react-resizable-panels": "^1.0.5",
    "server-only": "^0.0.1",
    "sonner": "^1.2.4",
    "tailwind-merge": "^2.0.0",
    "tailwindcss-animate": "^1.0.7",
    "use-debounce": "^10.0.0",
    "uuid": "^9.0.1",
    "valibot": "^0.22.0"
  },
  "devDependencies": {
    "@quramy/prisma-fabbrica": "^2.0.2",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/node": "^20.10.2",
    "@types/react": "^18.2.40",
    "@types/react-dom": "^18.2.17",
    "@types/uglify-js": "^3.17.4",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.1.0",
    "@vitest/ui": "^1.0.2",
    "autoprefixer": "^10.4.16",
    "dotenv-cli": "^7.3.0",
    "esbuild": "^0.19.10",
    "eslint": "^8.55.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-next": "14.0.3",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "happy-dom": "^12.10.3",
