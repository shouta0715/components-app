{
  "name": "component-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "docker compose up -d && next dev",
    "up": "docker compose up -d",
    "down": "docker compose down",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "generate": "pnpm drizzle-kit generate:mysql --config=drizzle.config.ts",
    "studio": "pnpm drizzle-kit studio --port 5555 --verbose",
    "seed": "dotenv -e .env.development.local pnpm tsx src/db/seed/index.ts",
    "migrate": "pnpm tsx src/db/migrate.ts",
    "prepare": "husky install",
    "format": "prettier --write . && eslint --fix .",
    "test": "dotenv -e .env.test pnpm vitest -c ./vitest.config.ts",
    "test:ui": "pnpm vitest -c ./vitest.config.ts --ui"
  },
  "dependencies": {
    "@auth/core": "^0.18.4",
    "@auth/drizzle-adapter": "^0.3.9",
    "@aws-sdk/client-s3": "^3.465.0",
    "@aws-sdk/s3-presigned-post": "^3.465.0",
    "@aws-sdk/s3-request-presigner": "^3.465.0",
    "@next/env": "^14.0.3",
    "@paralleldrive/cuid2": "^2.2.2",
    "@planetscale/database": "^1.11.0",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-toast": "^1.1.5",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "cmdk": "^0.2.0",
    "drizzle-orm": "^0.29.1",
    "lucide-react": "^0.294.0",
    "mysql2": "^3.6.5",
    "next": "14.0.3",
    "next-auth": "5.0.0-beta.4",
    "next-themes": "^0.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "server-only": "^0.0.1",
    "tailwind-merge": "^2.0.0",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@types/node": "^20.10.2",
    "@types/react": "^18.2.40",
    "@types/react-dom": "^18.2.17",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/ui": "^1.0.2",
    "autoprefixer": "^10.4.16",
    "drizzle-kit": "^0.20.6",
    "eslint": "^8.55.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-next": "14.0.3",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "happy-dom": "^12.10.3",
    "husky": "^8.0.3",
    "lint-staged": "^15.1.0",
    "postcss": "^8.4.32",
    "prettier": "^3.1.0",
    "tailwindcss": "^3.3.5",
    "tsx": "^4.6.2",
    "typescript": "^5.3.2",
    "vitest": "^1.0.2"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,scss,md,html}": [
      "prettier --write"
    ]
  }
}
