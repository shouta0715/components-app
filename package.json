    "tailwindcss": "^3.4.0",
    "tsx": "^4.6.2",
    "typescript": "^5.3.2",
    "vitest": "^1.0.2"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,scss,md,html}": [
      "prettier --write"
    ]
  },
  "prisma": {
    "seed": "tsx --no-cache prisma/seed/index.ts"
  }
}
